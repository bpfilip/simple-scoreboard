{
  "devDependencies": {
    "electron": "^15.1.0",
    "electron-installer-windows": "^3.0.0",
    "electron-packager": "^15.4.0",
    "electronmon": "^2.0.2"
  },
  "name": "simple-scoreboard",
  "version": "1.0.3",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "test": "electronmon .",
