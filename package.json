    "setup": "electron-installer-windows --src dist/simple-scoreboard-win32-x64/ --dest dist/installers/"
  },
  "author": "FBP_Coding",
  "license": "ISC",
  "description": "A simple to use scoreboard",
  "dependencies": {
    "cors": "^2.8.5",
    "electron-squirrel-startup": "^1.0.0",
    "express": "^4.17.1",
    "node-json-db": "^1.4.1",
    "simple-json-db": "^1.2.3",
    "socket.io": "^4.2.0"
  }
}
